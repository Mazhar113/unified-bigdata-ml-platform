{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3StateBucketAccess",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject",
        "s3:ListBucket",
        "s3:GetBucketVersioning",
        "s3:PutBucketVersioning",
        "s3:PutBucketEncryption",
        "s3:GetBucketLocation"
      ],
      "Resource": [
        "arn:aws:s3:::REPLACE_BUCKET_NAME",
        "arn:aws:s3:::REPLACE_BUCKET_NAME/*"
      ]
    },
    {
      "Sid": "DynamoDBLockTableAccess",
      "Effect": "Allow",
      "Action": [
        "dynamodb:PutItem",
        "dynamodb:GetItem",
        "dynamodb:DeleteItem",
        "dynamodb:DescribeTable",
        "dynamodb:Query",
        "dynamodb:UpdateItem"
      ],
      "Resource": [
        "arn:aws:dynamodb:REPLACE_REGION:REPLACE_ACCOUNT_ID:table/REPLACE_DYNAMO_TABLE"
      ]
    }
  ]
}
